#include <stdio.h>

int main()
{
   
   int i,pos,num,new_pos,old_pos;
   
   printf("Enter the number\n");
   scanf("%d",&num);
   
   printf("Enter Old and new position\n");
   scanf("%d%d",&old_pos,&new_pos);
   for(pos=31;pos>=0;pos--)
   {
       if(pos==old_pos)
       num=num|(1<<new_pos);
   }
   printf("after number=%d\n",num);
    printf("Hello World");

    return 0;
}
